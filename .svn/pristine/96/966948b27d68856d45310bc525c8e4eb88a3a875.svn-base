@import url('https://fonts.googleapis.com/css?family=Josefin+Slab:100,100i,300,300i,400,400i,600,600i,700,700i|Rubik:300,300i,400,400i,500,500i,700,700i,900,900i');

body {
	margin: 0;
	-webkit-text-size-adjust: 100%;
}
.wrapper {
	font-size: 14px;
	line-height: 23px;
	margin: 0;
	min-width: 320px;
	-webkit-text-size-adjust: 100%;
}
.wrapper p {margin: 0 0 14px;}

.wrapper form,
.wrapper fieldset {
	margin: 0;
	padding: 0;
	border-style: none;
}
.wrapper input,
.wrapper button {
	vertical-align: middle;
	font-size: 14px;
	line-height: 24px;
	-webkit-border-radius: 0;
	-moz-border-radius: 0;
	border-radius: 0;
	-webkit-box-shadow: inset rgba(0, 0, 0, 0) 0 0 0;
	-moz-box-shadow: inset rgba(0, 0, 0, 0) 0 0 0;
	box-shadow: inset rgba(0, 0, 0, 0) 0 0 0;
	-webkit-appearance: none;
}
.wrapper input::-moz-focus-inner,
.wrapper button::-moz-focus-inner {
	padding: 0;
	border: 0;
}
.wrapper button,
.wrapper input[type="button"],
.wrapper input[type="submit"] {
	cursor: pointer;
	-webkit-appearance: button;
}
.wrapper label,
.wrapper button,
.wrapper input[type="button"] {
	cursor: pointer;
}
.wrapper article,
.wrapper aside,
.wrapper details,
.wrapper figcaption,
.wrapper figure,
.wrapper footer,
.wrapper header,
.wrapper hgroup,
.wrapper menu,
.wrapper nav,
.wrapper section {
	display: block;
}
.wrapper figure {
	margin: 0;
}
.wrapper img {
	border: 0;
	border-style: none;
	vertical-align: top;
	-ms-interpolation-mode: bicubic;
	vertical-align: baseline;
	font-weight: inherit;
	font-family: inherit;
	font-style: inherit;
	font-size: 100%;
	border: 0 none;
	outline: 0;
	padding: 0;
	margin: 0;
}
.wrapper a,
.wrapper a:link,
.wrapper a:visited {
	color: #000;
	text-decoration: none;
}
.wrapper a:focus,
.wrapper a:hover {
	outline: none;
	outline: 0;
}
.wrapper strong {font-weight: 700;}

.text-center {
	display:table;
	position:relative;
	overflow:hidden;
	width:100%;
	height:100%;
}
.text-center .text-inner {
	display:table-cell;
	vertical-align:middle;
}
.text-center .text-inner .text-last {
	width:100%;
	display:block;
}
*+html .text-center {
	display:block;
	position:relative;
}
*+html .text-center .text-inner  {
	top: 50%;
	display:block;
	position: absolute;
	left:0;
}
*+html .text-center .text-inner .text-last {
	top: -50%;
	display: block;
	position: relative;
}
/*	   font-family   	*/
.wrapper {font-family: arial, helvetica, sans-serif;}
/*	   end font-family   	*/

/*	   colors   	*/
.wrapper,
#topnav > li > a,
.toogle-menu,
#search_box .search_btn,
.specification-list li:before,
.hold-tabs .elem label:hover,
.hold-tabs > input:nth-child(1):checked ~ .tab-menu .elem li:nth-child(1) label,
.hold-tabs > input:nth-child(2):checked ~ .tab-menu .elem li:nth-child(2) label,
.hold-tabs > input:nth-child(3):checked ~ .tab-menu .elem li:nth-child(3) label,
.hold-tabs > input:nth-child(4):checked ~ .tab-menu .elem li:nth-child(4) label,
.hold-tabs > input:nth-child(5):checked ~ .tab-menu .elem li:nth-child(5) label,
.topseller-wrap .col.large .price,
.faq-box .tab-menu .elem label,
.pay-foot .pay-info-list li:before,
.fade-gallery .close {color:#000;}

.close-menu,
.about-box .col-hold .col:first-child,
.tab-menu .elem label,
.category-box .category-row.up .col:last-child .row-down .box:last-child .title-category,
.fade-gallery label[for="modal"] > span:first-child,
.fade-gallery label.next, 
.fade-gallery label.prev,
.fade-gallery label.next .fa, 
.fade-gallery label.prev .fa,
.fade-gallery label[for="modal"] > span:first-child,
.fade-gallery .close {color:#fff;}

#topnav > li:hover > a,
.topseller-wrap .box .price,
.fade-gallery .gall-prev label:hover .fa,
.fade-gallery .gall-next label:hover .fa {color:#db1c5d;}

.topseller-box h2 {color:#e6e6e6;}

.topseller-wrap .box .price-old {color:#808080;}

.faq-box .hold-tabs .elem label:hover,
.faq-box .hold-tabs > input:nth-child(1):checked ~ .tab-menu .elem li:nth-child(1) label,
.faq-box .hold-tabs > input:nth-child(2):checked ~ .tab-menu .elem li:nth-child(2) label,
.faq-box .hold-tabs > input:nth-child(3):checked ~ .tab-menu .elem li:nth-child(3) label,
.faq-box .hold-tabs > input:nth-child(4):checked ~ .tab-menu .elem li:nth-child(4) label,
.faq-box .hold-tabs > input:nth-child(5):checked ~ .tab-menu .elem li:nth-child(5) label {color:#cccccc;}

.fade-gallery .close:hover{color: #a09797;}
/*	   end colors   	*/

/*	   backgrounds   	*/
#topnav > li > a:after,
.about-box ul li:before,
.hold-tabs .elem label:hover,
.hold-tabs > input:nth-child(1):checked ~ .tab-menu .elem li:nth-child(1) label,
.hold-tabs > input:nth-child(2):checked ~ .tab-menu .elem li:nth-child(2) label,
.hold-tabs > input:nth-child(3):checked ~ .tab-menu .elem li:nth-child(3) label,
.hold-tabs > input:nth-child(4):checked ~ .tab-menu .elem li:nth-child(4) label,
.hold-tabs > input:nth-child(5):checked ~ .tab-menu .elem li:nth-child(5) label,
.topseller-wrap .box .topseller-link,
.accordion ul li,
.fade-gallery,
.product-description .size-list li a {background:#fff;}

.toogle-menu > span,
.title-box h1:before,
.about-box .col-hold .col:first-child,
.topseller-box,
.tab-menu .elem label:hover:after,
.hold-tabs > input:nth-child(1):checked ~ .tab-menu .elem li:nth-child(1) label:after,
.hold-tabs > input:nth-child(2):checked ~ .tab-menu .elem li:nth-child(2) label:after,
.hold-tabs > input:nth-child(3):checked ~ .tab-menu .elem li:nth-child(3) label:after,
.hold-tabs > input:nth-child(4):checked ~ .tab-menu .elem li:nth-child(4) label:after,
.hold-tabs > input:nth-child(5):checked ~ .tab-menu .elem li:nth-child(5) label:after,
.topseller-wrap .box.title,
.regarding-box h2:before,
.regarding-box h2:after,
.accordion .box-accordion:before,
.accordion .box-accordion:after,
.info-foot .box ul li a:before,
.product-description .size-link:before {background:#000;}

.advantages-box .advantages-title:before {background:#e4e4e4;}

#header,
.children,
.product-description:before {background:#f5f5f5;}

#topnav > li:hover > a:after,
.category-box .category-row.up .col:last-child .row-down .box:last-child,
.fade-gallery label.next, 
.fade-gallery label.prev {background:#db1c5d;}

.title-box .title-list li:before {background:#cccccc;}

.tab-menu .elem label:after {background:#e6e6e6;}

.faq-box .tab-menu .elem label:hover:after,
.faq-box .hold-tabs > input:nth-child(1):checked ~ .tab-menu .elem li:nth-child(1) label:after,
.faq-box .hold-tabs > input:nth-child(2):checked ~ .tab-menu .elem li:nth-child(2) label:after,
.faq-box .hold-tabs > input:nth-child(3):checked ~ .tab-menu .elem li:nth-child(3) label:after,
.faq-box .hold-tabs > input:nth-child(4):checked ~ .tab-menu .elem li:nth-child(4) label:after,
.faq-box .hold-tabs > input:nth-child(5):checked ~ .tab-menu .elem li:nth-child(5) label:after {background:#dadada;}

.fade-gallery label[for="modal"] > span:first-child:after {background-color: #1788c6;}

.controls label{background:#e2e2e4;}
/*	   end backgrounds   	*/

/*	   borders   	*/
#search_box {border-bottom-color:#000;}

.accordion ul li {border-color:#e5e5e5;}

.category-box .title-category {border-color:#000;}

.thumbnail label:before {border-color:#db1c5d;}

.product-description .description-hold:nth-of-type(1) {border-bottom-color:#dcdcdc;}
/*	   end borders   	*/

.wrapper .container {
	max-width:1200px;
	margin:0 auto;
	padding:0 20px;
}
.logo {
	display:block;
	overflow: hidden;
	height:auto;
}
.logo {width:55px;}

.logo a {
	display:block;
	width:100%;
	height:100%;
	-webkit-transition: opacity .2s ease-in-out;
	-moz-transition: opacity .2s ease-in-out;
	-ms-transition: opacity .2s ease-in-out;
	-o-transition: opacity .2s ease-in-out;
	transition: opacity .2s ease-in-out;
}
.logo a:hover {opacity:0.7;}

.logo img {
	display:block;
	width:100%;
	height:auto;
	-webkit-background-size: cover;
	-o-background-size: cover;
	-moz-background-size: cover;
	background-size: cover;
}
h1,
h2 {
	font-size:60px;	line-height:60px;
	font-weight:700;
	text-transform:uppercase;
	position:relative;
	margin:0 0 11px;
}
h3 {
	font-size:14px;	line-height:18px;
	font-weight:700;
	margin:0 0 13px;
}
.col-hold {display:flex;}

.col-hold .col {
	width:50%;
	padding:0 98px;
	-webkit-box-sizing: border-box;	-moz-box-sizing: border-box;	box-sizing: border-box;
}
.col-hold .col:first-child {padding:0 98px 0 0;}

/*	   header   	*/
#header {
	position:relative;
	width:100%;
	background-size:cover;
	padding:0;
	margin:0 0 40px;
}
#header:before {	content:'';	background: url(../images/header-before.png) no-repeat 0 0;
	background-size:100% 100%;	width: 100%;	height: 40px;	position:absolute;	top:100%;	left:0;}
.header-wrap {
	display:flex;
	align-items:center;
	justify-content: space-between;
	padding:24px 0 0;
}
.logo-wrap {padding:0 10px 0 0;}

/*	   navigation   	*/
.open-check {display:none;}

#nav{
	width: 100%;
	z-index: 10;
	position:relative;
}
#nav:after {
	content:"";
	display:block;
	clear:both;
}
#topnav {
	text-align:center;
	list-style:none;
	margin:0;
	padding:0;
	font-size:0;
	line-height:0;
	position:relative;
}
#topnav:after {
	content:"";
	display:block;
	clear:both;
}
#topnav > li {
	display:inline-block;
	vertical-align:top;
	font-size:12px;
	line-height:14px;
	padding:0 14px;
}
#topnav > li:last-child {border: none;}

#topnav > li > a {
	padding:0 14px;
	font-weight: 700;
	text-transform:uppercase;
	display:block;
	text-decoration: none;
	position:relative;
	-webkit-transition: color .2s ease-in-out;	-moz-transition: color .2s ease-in-out;	transition: color .2s ease-in-out;
}

#topnav > li:hover > .open-check + a:before {border-color: #db1c5d transparent transparent transparent;}

#topnav > li > .open-check + a:before {	content:'';	width: 0;
	height: 0;
	border-style: solid;
	border-width: 4px 4px 0 4px;
	border-color: #000000 transparent transparent transparent;	position:absolute;	top:50%;	right:0;
	-webkit-transform: translateY(-50%);	-moz-transform: translateY(-50%);	-ms-transform: translateY(-50%);	-o-transform: translateY(-50%);	transform: translateY(-50%);
	-webkit-transition: border-color .2s ease-in-out;	-moz-transition: border-color .2s ease-in-out;	transition: border-color .2s ease-in-out;}
#topnav > li > a:after {	content:'';	width: 1px;	height: 50px;	position:absolute;	bottom:32px;	left:50%;
	-webkit-transform: translateX(-50%);	-moz-transform: translateX(-50%);	-ms-transform: translateX(-50%);	-o-transform: translateX(-50%);	transform: translateX(-50%);
	-webkit-transition: background .2s ease-in-out;	-moz-transition: background .2s ease-in-out;	transition: background .2s ease-in-out;}
.children {
	position:absolute;
	top: 100%;
	left:0;
	z-index: 100;
	text-align:left;
	width:100%;
	list-style: none;
	display:block;
	padding: 20px 10px 10px 20px;
	opacity:0;
	visibility: hidden;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transition: opacity .2s ease, visibility .2s ease;
	-moz-transition: opacity .2s ease, visibility .2s ease;
	-ms-transition: opacity .2s ease, visibility .2s ease;
	-o-transition: opacity .2s ease, visibility .2s ease;
	transition: opacity .2s ease, visibility .2s ease;
}
#topnav > li:hover .children {
	opacity:1;
	visibility: visible;
}
.children > li {
	margin: 0 0 20px;
	display:inline-block;
	vertical-align:top;
	width: 24.5%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	position:relative;
}
.children > li.children-img {text-align: right;}

.children > li.children-img img {
	width:250px;
	height:auto;
}
.children2 {
	list-style: none;
	padding: 20px 0 0;
	margin:0;
}
.children2 li {
	position:relative;
	margin: 0 0 10px;
}
.children2 > li:before {
	content: '';
	position:absolute;
	top: 50%;
	left:0;
	margin: -2px 0 0;
	height: 4px;
	width: 4px;
	background: #fff;
}
.children > li > a,
.children2 > li > a {
	text-decoration:none;
	position:relative;
	font-weight:700;
	font-size:12px;
	line-height:14px;
	color: #000;
}
.children > li > a:before,
.children2 > li > a:before {
	content:'';
	position:absolute;
	bottom:0;
	left:0;
	width:100%;
	height: 1px;
	background: #000;
	-webkit-transform: scale(0);
	-moz-transform: scale(0);
	-ms-transform: scale(0);
	-o-transform: scale(0);
	transform: scale(0);
	-webkit-transition: transform .2s ease-in-out;
	-moz-transition: transform .2s ease-in-out;
	-ms-transition: transform .2s ease-in-out;
	-o-transition: transform .2s ease-in-out;
	transition: transform .2s ease-in-out;
}
.children > li > a:hover:before,
.children2 > li > a:hover:before {
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-ms-transform: scale(1);
	-o-transform: scale(1);
	transform: scale(1);
}
#topnav > li > .open-check ~ .children > li:nth-child(1),
#topnav > li > .open-check ~ .children > li:nth-child(2),
.children > li > .open-check ~ .children2 > li:nth-child(1),
.children > li > .open-check ~ .children2 > li:nth-child(2) {
	display:none;
}
#topnav > li > label,
.children > li > label{display:none;}

.hold-toogle {
	float: right;
	display:none;
}
.toogle-menu {
	z-index:61;
	height:17px;
	position:relative;
	cursor: pointer;
	display:none;
	padding: 0 35px 0 0;
	line-height: 17px;
}
.toogle-menu > span {
	position:absolute;
	right:0;
	height:3px;
	width:30px;
}
.toogle-menu .t {top: 0;}
.toogle-menu .c {top: 7px;}
.toogle-menu .b {top: 14px;}
.navigation-main {
	display:block;
	padding-top: 4px;
}
.close-menu {
	display:none;
	padding: 15px 30px 15px 10px;
	background: #000;
	text-transform: uppercase;
	font-weight:bold;
	cursor:pointer;
	font-size:14px;
	line-height:16px;
	position:relative;
}
.overlay {display:none;}
/*	   end navigation   	*/

#search_box .search-link{ 
	display:block;
	text-decoration:none;
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height: 100%;
	z-index:2;
}
#search_box { 
	padding: 0;
	width: 100px;
	position: relative;
	float: right;
	border-bottom:1px;
	border-bottom-style:solid;
	margin:0 0 -2px;
	padding:0 0 2px 10px;
}
.wrapper input[type="text"],
.wrapper input[type="password"],
.wrapper .text{
	width:100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	font-size:12px;	line-height:14px;
	display:block;
	height: 14px;
	padding: 0 20px 0 0;
	color:#000;
	margin:0;
	background:none;
	outline: none;
	outline: 0;
	border: none;
	-webkit-transition: border-color .2s ease-in-out;
	-moz-transition: border-color .2s ease-in-out;
	transition: border-color .2s ease-in-out;
}
#search_box .search_btn{
	position:absolute;
	top:0px;
	right:0px;
	height: 14px;
	width:14px;
	text-align:center;
	background:none;
	overflow:hidden;
	padding: 0;
	border:0;
	margin:0;
	cursor:pointer;
}
#search_box .search_btn .fa{
	font-size:12px;	line-height:14px;
	vertical-align:top;
	-webkit-transition: opacity .2s ease-in-out;
	-moz-transition: opacity .2s ease-in-out;
	transition: opacity .2s ease-in-out;
}
#search_box .search_btn:hover .fa,
#search_box .search-link:hover .fa{ opacity: 0.8;}

/*	   main   	*/
#main {
	overflow:hidden;
	width:100%;
}
.title-box {
	display:flex;
	justify-content: space-between;
	width:100%;
	margin:0 0 81px;
	padding:43px 0 0;
}
.title-box h1:before {	content:'';	width: 9999999px;	height: 7px;	position:absolute;	top:25px;	right:105%;}
.title-box .title-wrap {max-width:500px;}

.title-box .title-hold {
	display:block;
	font-size:11px;	line-height:13px;
	text-transform:uppercase;
}
.title-box .title-list {
	list-style:none;	margin:0;	padding:6px 0 0;
	text-align:right;
}
.title-box .title-list li {
	font-size:11px;	line-height:13px;
	margin:0 0 17px;
	position:relative;
}
.title-box .title-list li:before {	content:'';	width: 999999px;	height: 1px;	position:absolute;	top:4px;	left:120%;}
/*	   product gallery   	*/
.product-box {position: relative;}

.product-images {
	width: 100%;
	position: relative;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.product-images:after {	content:'';	display:block;	clear:both;}
/*	   fade gallery   	*/
.open-check {display: none;}
.product-box:after {
  content: '';
  display: block;
  clear: both;
}
.fade-gallery {
  margin: 0;
  width:100%;
  font-size: 0;
  line-height: 0;
  white-space: nowrap;
  position: relative;
  text-align: center;
  z-index: 1;
  padding: 0 0 80px;
}
.fade-gallery:after {	content:'';	display:block;	clear:both;}
.fade-gallery label {cursor: pointer;}

.fade-gallery .holder-img {
  width: 100%;
  z-index: 1;
  position: relative;
  height: 292px;
  text-align: center;
  padding:0 70px;
  -webkit-box-sizing: border-box;  -moz-box-sizing: border-box;  box-sizing: border-box;
}
.fade-gallery .holder-img img {
  margin: 0 auto;
  max-width: 100%;
  max-height: 100%;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  transform: translateY(-50%);
  position: absolute;
  left: -99999px;
  opacity: 1;
  -webkit-transition: transform 0.2s ease-in-out;
  -moz-transition: transform 0.2s ease-in-out;
  -ms-transition: transform 0.2s ease-in-out;
  -o-transition: transform 0.2s ease-in-out;
  transition: transform 0.2s ease-in-out;
}
.fade-gallery > .open-check:nth-of-type(1):checked ~ .holder-img img:nth-of-type(1),
.fade-gallery > .open-check:nth-of-type(2):checked ~ .holder-img img:nth-of-type(2),
.fade-gallery > .open-check:nth-of-type(3):checked ~ .holder-img img:nth-of-type(3),
.fade-gallery > .open-check:nth-of-type(4):checked ~ .holder-img img:nth-of-type(4),
.fade-gallery > .open-check:nth-of-type(5):checked ~ .holder-img img:nth-of-type(5),
.fade-gallery > .open-check:nth-of-type(6):checked ~ .holder-img img:nth-of-type(6),
.fade-gallery > .open-check:nth-of-type(7):checked ~ .holder-img img:nth-of-type(7),
.fade-gallery > .open-check:nth-of-type(8):checked ~ .holder-img img:nth-of-type(8),
.fade-gallery > .open-check:nth-of-type(9):checked ~ .holder-img img:nth-of-type(9),
.fade-gallery > .open-check:nth-of-type(10):checked ~ .holder-img img:nth-of-type(10),
.fade-gallery > .open-check:nth-of-type(11):checked ~ .holder-img img:nth-of-type(11),
.fade-gallery > .open-check:nth-of-type(12):checked ~ .holder-img img:nth-of-type(12),
.fade-gallery > .open-check:nth-of-type(13):checked ~ .holder-img img:nth-of-type(13),
.fade-gallery > .open-check:nth-of-type(14):checked ~ .holder-img img:nth-of-type(14),
.fade-gallery > .open-check:nth-of-type(15):checked ~ .holder-img img:nth-of-type(15) {
  position: relative;
  left: 0;
}
.fade-gallery label[for="modal"] {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
  font-size: 14px;
