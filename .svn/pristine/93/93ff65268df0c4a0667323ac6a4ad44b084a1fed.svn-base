.accordion {
  width: 100%;
  min-height: 0;
  display: block;
  position: relative;
  padding: 0 0 1px;
  overflow: hidden
}
.accordion .box-accordion{
	display: block;
	position: relative;
	font-size:14px;	line-height:18px;
	font-weight:700;
  margin: 0;
  padding:15px 0 13px;
  cursor: pointer;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.accordion ul li .accordion-text {
  position: relative;
  overflow: hidden;
  max-height: 0;
  -webkit-transition: max-height 0.5s ease-in-out;
  -moz-transition: max-height 0.5s ease-in-out;
  -o-transition: max-height 0.5s ease-in-out;
  transition: max-height 0.5s ease-in-out;
}
.accordion ul li input{display: none;}

.accordion ul li input:checked ~ .accordion-text {
  max-height: 1000px;
}
.accordion-text .box-text{padding:0 0 11px;}

.accordion ul {
  list-style: none;
  padding: 0;
  margin: 0;
}
.accordion ul li {
  position: relative;
  padding: 0 20px;
  margin: 0;
  border:1px;
  border-style:solid;
  border-color:#e5e5e5;
  margin:0 0 -1px;
  -webkit-box-sizing: border-box;  -moz-box-sizing: border-box;  box-sizing: border-box;
}
.accordion .box-accordion:before,
.accordion .box-accordion:after{
  content: ""
  position: absolute;
  transform: translate(-6px, 0);
  top: 20px;
  right: 6px;
}
.accordion .box-accordion:before,
.accordion .box-accordion:after {
  content: "";
  position: absolute;
  width: 3px;
  height: 9px;
  -webkit-transition: transform 0.5s ease-in-out;
  -moz-transition: transform 0.5s ease-in-out;
  -o-transition: transform 0.5s ease-in-out;
  transition: transform 0.5s ease-in-out;
}
.accordion .box-accordion:before {
  transform: translate(2px, 0) rotate(45deg);
}
.accordion .box-accordion:after {
  transform: translate(-2px, 0) rotate(-45deg);
}
.accordion ul li input:checked ~ .box-accordion:before {
  transform: translate(-2px, 0) rotate(45deg);
}
.accordion ul li input:checked ~ .box-accordion:after {
  transform: translate(2px, 0) rotate(-45deg);
}
